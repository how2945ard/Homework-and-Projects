<%= link_to '<i class="fa fa-angle-double-left"></i> Go back'.html_safe, @parent_folder.url_string , :class=>"btn btn-outline btn-primary", :style=>'margin:30px;' %>
<%= form_for @file, url: {action: "create", f_id: @parent_folder.id}, html: {multipart: true} do |f| %>
  <div class="form-group">
    <label>File name</label>
    <%= f.text_field :name,:class=>'form-control' %>
  </div>
  <div class="form-group">
    <label>Choose file</label>
    <%= f.file_field :file_obj %>
  </div>
  <%= f.submit "Upload", :class=>"btn btn-default" %>
<% end %>